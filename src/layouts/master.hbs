<!doctype html>
<html lang="en" class="no-js">
  <head>

		{{#block "master__head"}}
			{{>head/head-meta}}
			{{>head/head-icons}}
			{{>head/head-css}}
			{{>head/head-fonts}}
			{{>head/head-scripts}}
		{{/block}}

  </head>
  <body class="body">

    <div class="content">
      {{#block "master__header"}}
        {{> header}}
      {{/block}}

      <div id="page" class="page{{#if pageData.pageClass}} {{pageData.pageClass}}{{/if}}">
        <main class="main" role="main">
        {{#block "master__main"}}{{/block}}
        </main>
      </div>

      {{#block "master__footer"}}
        {{> footer}}
      {{/block}}
    </div>

    {{#block "master__foot"}}
			{{>foot/foot-scripts}}
    {{/block}}

  </body>
</html>
